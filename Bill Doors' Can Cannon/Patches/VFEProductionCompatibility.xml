<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Vanilla Furniture Expanded - Production</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAdd">
          <xpath>
            /Defs/ThingDef[@Name="AmmoERationmmBase"]/tradeTags
          </xpath>
          <value>
            <li>CE_AutoEnableCrafting_VFE_TableStoveLarge</li>
          </value>
        </li>

        <li Class="PatchOperationAdd">
          <xpath>Defs/RecipeDef[defName = "CookMealERation"]/recipeUsers</xpath>
          <value>
            <li>VFE_TableStoveLarge</li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>

</Patch>